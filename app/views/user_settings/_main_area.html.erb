	<div id="resume05" class = "content3">

		<div class="content-group">
			<div class="controls control-title1" style="width:100%; float:left;">
				<p class="text-info" style="padding-left:0px;">
					Reset Password
				</p>
			</div>
		</div>
		<div class="content-group" style="width:100%;">
			<div class="line3">

			</div>
		</div>
		<div class="content-group">
			<ul class="display-ul">
				<%= form_for( @user, :url => { :action => "update_password" },:method => "PUT" ) do |f| %>
	  				<li class="li-inline">
						<label><small>Current</small></label>						
						<%= f.password_field :current_password, :autocomplete => "off",:id=>"current_password",:class=>"input-small"  %>
					</li>
					<li class="li-inline">
						<label><small>New</small></label>						
						<%= f.password_field :password, :autocomplete => "off",:id=>"new_password",:class=>"input-small"  %>
					</li>   			
	    			<li class="li-inline">
						<label><small>Confirm</small></label>						
						<%= f.password_field :password_confirmation, :autocomplete => "off",:id=>"confirm_password",:class=>"input-small"  %>
					</li>	  			
	  				
	  				<li class="li-inline">						
							<%= f.submit "Reset",:class=> "btn btn-primary" %>						
					</li>				
	  			
				<% end %>
			</ul>			
		</div>
	</div>
	<div id="resume06" class = "content3">

