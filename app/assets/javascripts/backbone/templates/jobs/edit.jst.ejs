<h1>Edit job</h1>

<form id="edit-job" name="job">
  <div class="field">
    <label for="title"> title:</label>
    <input type="text" name="title" id="title" value="<%= title %>" >
  </div>

  <div class="field">
    <label for="description"> description:</label>
    <input type="text" name="description" id="description" value="<%= description %>" >
  </div>

  <div class="field">
    <label for="city"> city:</label>
    <input type="text" name="city" id="city" value="<%= city %>" >
  </div>

  <div class="field">
    <label for="state"> state:</label>
    <input type="text" name="state" id="state" value="<%= state %>" >
  </div>

  <div class="field">
    <label for="zip"> zip:</label>
    <input type="text" name="zip" id="zip" value="<%= zip %>" >
  </div>

  <div class="field">
    <label for="field"> field:</label>
    <input type="text" name="field" id="field" value="<%= field %>" >
  </div>

  <div class="field">
    <label for="tag"> tag:</label>
    <input type="text" name="tag" id="tag" value="<%= tag %>" >
  </div>

  <div class="field">
    <label for="visibility"> visibility:</label>
    <input type="text" name="visibility" id="visibility" value="<%= visibility %>" >
  </div>

  <div class="field">
    <label for="logo"> logo:</label>
    <input type="text" name="logo" id="logo" value="<%= logo %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Job" />
  </div>

</form>

<a href="#/index">Back</a>
